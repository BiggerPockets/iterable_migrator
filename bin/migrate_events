#!/usr/bin/env ruby

require "bundler"
require_relative "../lib/iterable_migrator"

printf "WARNING - This will migrate event records from Redshift to Iterable. Are you sure? N/y"
prompt = STDIN.gets.chomp
return unless prompt == "y"

ChangedAccountLevel.limit(1000).migrate
FirstTimePlus.limit(1000).migrate
FirstTimePremium.limit(1000).migrate
FirstTimePro.limit(1000).migrate
MortgageAffiliateLinkClicked.limit(1000).migrate
RealEstateAgentLeadSubmitted.limit(1000).migrate
RegisteredForWebinar.limit(1000).migrate
RenewalScore.limit(1000).migrate

Page.migrate
